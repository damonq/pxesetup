MENU TITLE Stock Installs Menu

LABEL Main Menu
  KERNEL vesamenu.c32
  APPEND pxelinux.cfg/default

LABEL Centos 6 Graphical
  KERNEL centos6/i386/vmlinuz
  APPEND initrd=centos6/i386/initrd.img ramdisk_size=9216 jfs xfs

