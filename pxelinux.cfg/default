LABEL Boot local hard drive
  LOCALBOOT 0

LABEL Rescue
  KERNEL vesamenu.c32
  APPEND pxelinux.cfg/rescue

LABEL Tools
  KERNEL vesamenu.c32
  APPEND pxelinux.cfg/tools

LABEL Stock Installs
  KERNEL vesamenu.c32
  APPEND pxelinux.cfg/stock

LABEL Automatic Installs
  KERNEL vesamenu.c32
  APPEND pxelinux.cfg/auto
